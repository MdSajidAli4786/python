   "source": [
    "#Armstrong number\n",
    "num = int(input(\"enter a number :\"))\n",
    "var=num\n",
    "sum = 0\n",
    "while num>0 :\n",
    "    n = num%10\n",
    "    sum += n**3\n",
    "    num = num//10\n",
    "if var==sum :\n",
    "    print(\"The Number is An Armstrong Number\",sum)\n",
    "else :\n",
    "    print(\"Not An Armstrong Number\",sum)"
   ]
